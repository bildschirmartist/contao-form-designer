{
  "name":"netzmacht/contao-form-designer",
  "description":"Contao form designer allows custom form styles",
  "keywords":["form", "contao"],
  "type":"contao-bundle",
  "license":"LGPL-3.0-or-later",
  "authors":[
    {
      "name":"David Molineus",
      "email":"mail@netzmacht.de",
      "homepage":"https://netzmacht.de",
      "role":"Developer"
    }
  ],
  "support":{
    "email":"mail@netzmacht.de",
    "issues":"https://github.com/netzmacht/contao-form-designer/issues",
    "source":"https://github.com/netzmacht/contao-form-designer"
  },
  "require":{
    "php":">=7.1",
    "contao/core-bundle":"~4.4",
    "contao-community-alliance/meta-palettes":"^2.0 || ^1.11",
    "netzmacht/html": "~2.0",
    "menatwork/contao-multicolumnwizard": "^3.3.14"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.1",
    "phpcq/all-tasks": "^1.2"
  },
  "autoload": {
    "psr-4": { "Netzmacht\\Contao\\FormDesigner\\": "src/" }
  },
  "autoload-dev": {
    "files": [
      "vendor/phpcq/autoload-validation/hacks/contao-hack.php"
    ]
  },
  "extra":{
    "contao-manager-plugin": "Netzmacht\\Contao\\FormDesigner\\ContaoManager\\Plugin",
    "branch-alias": {
      "dev-master": "1.1.x-dev",
      "dev-develop": "1.2.x-dev"
    }
  }
}
